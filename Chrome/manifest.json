{
    "name": "Facebook Friend",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "Add auto list friend and unlock freind",
    "icons": {
        "16": "browseraction/icons/16x16.png",
        "48": "browseraction/icons/48x48.png",
        "128": "browseraction/icons/128x128.png"
    },
    "omnibox": {
        "keyword": "yeah"
    },
    "browser_action": {
        "default_icon": {
            "19": "browseraction/icons/19x19.png",
            "38": "browseraction/icons/38x38.png"
        },
        "default_title": "Facebook tools",
        "default_popup": "browseraction/popup.html"
    },
    "background": {
        "scripts": [
            "browseraction/background.js"
        ],
        "persistent": false
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "permissions": [
        "activeTab",
        "system.cpu",
        "storage",
        "system.memory",
        "background",
        "http://*/*",
        "https://*/*"
    ]
}